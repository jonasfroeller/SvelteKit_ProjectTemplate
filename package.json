{
	"version": "1.0.0",
	"name": "sveltekit-projecttemplate",
	"description": "project template for sveltekit",
	"private": true,
	"author": "Jonas Froeller",
	"homepage": "https://svelte-kit-project-template.vercel.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonasfroeller/SvelteKit_ProjectTemplate"
	},
	"keywords": [
		"template",
		"svelte",
		"svelte-kit"
	],
	"license": "MIT",
	"funding": "https://buymeacoffee.com/jonasfroeller",
	"scripts": {
		"all": "npm-run-all --parallel typesafe-i18n dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"fcl": "npm-run-all format check lint",
		"typesafe-i18n": "typesafe-i18n",
		"commit": "cz"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/html-minifier": "^4.0.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.34.0",
		"html-minifier": "^4.0.0",
		"npm-run-all": "^1.8.0",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"rtlcss": "^4.1.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"sveltekit-superforms": "^1.8.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.0",
		"typescript": "^5.2.2",
		"vite": "^5.0.0",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"bits-ui": "^0.8.3",
		"clsx": "^2.0.0",
		"lucide-svelte": "^0.268.0",
		"mode-watcher": "^0.1.2",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.17",
		"tailwindcss-animate": "^1.0.7",
		"typesafe-i18n": "^5.26.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
